<?xml version="1.0" encoding="UTF-8"?>
<mime-info>
  <mime-type type="application/x-waag-dosa-excel">
    <sub-class-of type="application/vnd.ms-excel" />
    <acronym>DOSAXLS</acronym>
    <_comment>
      Dienst onderzoek en statistiek (Amsterdam) Excel
    </_comment>
    <glob pattern="*/files/dosa/*.xls" />
  </mime-type>

  <mime-type type="text/turtle">
    <acronym>Turtle</acronym>
    <_comment>RDF Turtle</_comment>
    <glob pattern="*.ttl" />
  </mime-type>

  <!-- UitBase v3 -->
  <mime-type type="application/x-waag-uitbase-v3+xml">
    <sub-class-of type="application/xml" />
    <acronym>NUBXML</acronym>
    <_comment>NUB UitBase V3</_comment>
    <root-XML localName="nubxml" />
  </mime-type>

  <!-- UitBase v4 -->
  <mime-type type="application/x-waag-uitbase-v4+xml">
    <sub-class-of type="application/xml" />
    <acronym>NUBXML</acronym>
    <_comment>NUB UitBase V4</_comment>
    <glob pattern="http://*.uitburo.nl/api/*" />   
    <!-- <root-XML localName="search"/> -->
  </mime-type>

  <!-- NBTC -->
  <mime-type type="application/x-waag-nbtc-pois+xml">
    <sub-class-of type="application/xml" />
    <acronym>NBTCXML</acronym>
    <_comment>
      Nederlands Bureau voor Toerisme en Congressen POI XML
    </_comment>
    <glob pattern="*/files/nbtc/ArtsHollandPois-*.xml" />
  </mime-type>
  
	<!-- TAM Export -->
  <mime-type type="application/x-waag-tam+xml">
    <sub-class-of type="application/xml" />
    <acronym>TAMXML</acronym>
    <_comment>TAM XML Export</_comment>
    <glob pattern="*acc.artsholland.com/api*" />    
  </mime-type>
</mime-info>