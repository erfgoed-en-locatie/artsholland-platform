<?xml version="1.0" encoding="UTF-8"?>
<mime-info>
  <mime-type type="application/x-waag-dosa-excel">
    <sub-class-of type="application/vnd.ms-excel"/>
    <acronym>DOSAXLS</acronym>
    <_comment>Dienst onderzoek en statistiek (Amsterdam) Excel</_comment>
    <glob pattern="http://127.0.0.1/ah/dos/*.xls"/>
  </mime-type>
  
   <!-- UitBase v3 -->
  <mime-type type="application/x-waag-uitbase-v3+xml">
    <sub-class-of type="application/xml"/>
    <acronym>NUBXML</acronym>
    <_comment>NUB UitBase V3</_comment>
    <root-XML localName="nubxml"/>
  </mime-type>
  
  <!-- UitBase v4 -->
  <mime-type type="application/x-waag-uitbase-v4-event+xml">
    <sub-class-of type="application/xml"/>
    <acronym>NUBXML</acronym>
    <_comment>NUB UitBase V4</_comment>
    <root-XML localName="event"/>
    <root-XML localName="events"/>
  </mime-type>
  <mime-type type="application/x-waag-uitbase-v4-production+xml">
    <sub-class-of type="application/xml"/>
    <acronym>NUBXML</acronym>
    <_comment>NUB UitBase V4</_comment>
    <root-XML localName="production"/>
    <root-XML localName="productions"/>
  </mime-type>
  <mime-type type="application/x-waag-uitbase-v4-location+xml">
    <sub-class-of type="application/xml"/>
    <acronym>NUBXML</acronym>
    <_comment>NUB UitBase V4</_comment>
    <root-XML localName="location"/>
    <root-XML localName="locations"/>
  </mime-type>
  <mime-type type="application/x-waag-uitbase-v4-group+xml">
    <sub-class-of type="application/xml"/>
    <acronym>NUBXML</acronym>
    <_comment>NUB UitBase V4</_comment>
    <root-XML localName="group"/>
    <root-XML localName="groups"/>
  </mime-type>
    
  <mime-type type="text/turtle">
    <acronym>Turtle</acronym>
    <_comment>RDF Turtle</_comment>
    <glob pattern="*.ttl"/>
  </mime-type>
</mime-info>