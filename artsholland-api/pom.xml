	</parent>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.2</version>
				<configuration>
					<!-- TODO: Move this to EAR module as it is a deployment thang. -->
					<packagingExcludes>
						WEB-INF/lib/artsholland-core-*.jar,
						WEB-INF/lib/sesame-*.jar,
						WEB-INF/lib/alibaba-*.jar
					</packagingExcludes>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.jvnet.jaxb2.maven2</groupId>
				<artifactId>maven-jaxb2-plugin</artifactId>
				<version>0.8.1</version>
				<executions>
					<execution>
						<goals>
							<goal>generate</goal>
						</goals>
						<configuration>
							<args>
								<arg>-extension</arg>
								<arg>-Xinheritance</arg>
							</args>
							<schemaDirectory>src/main/webapp/WEB-INF/xsd</schemaDirectory>
							<generatePackage>org.waag.ah.api</generatePackage>
							<removeOldOutput>false</removeOldOutput>
							<verbose>true</verbose>
							<plugins>
