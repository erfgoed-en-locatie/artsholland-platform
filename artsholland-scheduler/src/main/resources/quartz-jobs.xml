    <trigger>
      <cron>
        <name>SocialMediaEnricherTrigger</name>
        <job-name>SocialMediaEnricher</job-name>
        <job-group>EnricherService</job-group>
        <cron-expression>0 0/5 * * * ?</cron-expression>
        <time-zone>Europe/Amsterdam</time-zone>
