<persistence xmlns="http://java.sun.com/xml/ns/persistence"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
    version="2.0">
  <persistence-unit name="MyPersistenceUnit" transaction-type="RESOURCE_LOCAL">
      <provider>com.clarkparsia.empire.spi.JPAPersistenceProvider</provider>
      <properties>
        <property name="factory" value="sesame"/>
        <property name="url" value="http://localhost:8080/openrdf-sesame"/>
        <property name="repo" value="rest-incoming"/>
      </properties>
  </persistence-unit>
    
  <!-- <persistence-unit name="sesame" transaction-type="RESOURCE_LOCAL">
      <provider>com.clarkparsia.empire.spi.JPAPersistenceProvider</provider>
  </persistence-unit> -->
</persistence>