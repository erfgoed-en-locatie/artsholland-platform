            <owl:someValuesFrom>
              <owl:Class rdf:ID="Venue"/>
            </owl:someValuesFrom>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="atLocation"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <rdf:Property rdf:ID="hasProduction"/>
            </owl:onProperty>
            <owl:someValuesFrom>
              <owl:Class rdf:ID="Production"/>
            </owl:someValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="hasGenre"/>
            </owl:onProperty>
            <owl:allValuesFrom rdf:resource="#Genre"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </rdfs:subClassOf>
